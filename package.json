{
  "name": "valite",
  "version": "0.5.0",
  "description": "Concurrently execute your validators in a simple, practical and light validator engine.",
  "cdn": "dist/valite.js",
  "main": "dist/valite.cjs.js",
  "types": "types/index.d.ts",
  "module": "dist/valite.es.js",
  "files": [
    "dist/",
    "types/",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "pretest": "npm run build",
    "test": "tsc test/index.ts --lib es2018 && ava",
    "build": "tsc && bili src/index.js --formats cjs,umd,es --js buble --banner"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitorLuizC/valite.git"
  },
  "keywords": [
    "validate",
    "validator",
    "check",
    "checker"
  ],
  "author": {
    "url": "https://vitorluizc.github.io/",
    "name": "Vitor Luiz Cavalcanti",
    "email": "vitorluizc@outlook.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VitorLuizC/valite/issues"
  },
  "homepage": "https://github.com/VitorLuizC/valite#readme",
  "devDependencies": {
    "@types/get-value": "^3.0.0",
    "@types/node": "^10.10.1",
    "@types/set-value": "^2.0.0",
    "ava": "^0.25.0",
    "bili": "^3.1.2",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "get-value": "^3.0.1",
    "set-value": "^3.0.0"
  }
}
